{
    "name": "laravel-frontend-presets/karma-eslint",
    "description": "Laravel 5.5.x|5.6.x Front-end preset for Karma & ESLint (Unit Testing Vue Component, Code Coverage, ESLint, Babel...)",
    "keywords": ["laravel", "preset", "karma","eslint","coverage","testing"],
    "license": "MIT",
    "require": {
        "laravel/framework": "5.5.*|5.6.*"
    },
    "autoload": {
        "psr-4": {
            "LaravelFrontendPresets\\KarmaEslintPreset\\": "src/"
        }
    },
    "extra": {
      "laravel": {
        "providers": [
          "LaravelFrontendPresets\\KarmaEslintPreset\\KarmaEslintPresetServiceProvider"
       ]
      }
    }

}
